{
  "name": "static",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "dev:full": "astro dev --force",
    "build": "astro build",
    "build:full": "astro build --force",
    "preview": "astro preview",
    "sync": "astro sync",
    "typecheck": "astro check",
    "lint": "eslint .",
