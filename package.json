    "online:pages": "pnpx wrangler@latest pages deploy"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.7",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/ts-plugin": "^1.10.5",
    "@playform/compress": "^0.2.0",
    "astro": "^5.14.7",
    "lightningcss": "^1.30.2",
    "sharp": "^0.34.4",
    "terser": "^5.44.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@eslint/css": "^0.13.0",
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-interactive": "^12.0.0",
    "eslint-mdx": "^3.6.2",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.6.2",
    "jiti": "^2.6.1",
    "typescript-eslint": "^8.46.2"
  },
  "pnpm": {
    "overrides": {
      "html-minifier-terser": "npm:html-minifier-next",
      "vite": "^7.1.11"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  },
  "packageManager": "pnpm@10.18.3"
}
